generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Worker {
  id         Int     @id @default(autoincrement())
  name       String
  skill      String
  experience Int?
  location   String
  rating     Float?
  categories String  @db.Text
  summary    String? @db.Text
  avatar     String?
  phone      String?
  lat        Float?
  lng        Float?
}
